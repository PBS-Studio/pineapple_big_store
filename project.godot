config_version=5

[application]

config/name="PineappleBigStore"
config/name_localized={
"zh_TW": "鳳梨大學店"
}
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.776471, 0.639216, 0.180392, 1)
boot_splash/image="res://assets/custom/logo_large.png"
boot_splash/fullsize=false
config/icon="res://assets/custom/logo_large.png"

[autoload]

Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"fy": "res://undertale_fight/story/fy.dch",
"narrate": "res://undertale_fight/story/narrate.dch"
}
